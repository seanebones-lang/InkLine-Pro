{
  "name": "inklinepro",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0 || echo 'ESLint not configured'",
    "lint:fix": "eslint . --ext .ts,.tsx --fix || echo 'ESLint not configured'",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "typecheck": "tsc --noEmit",
    "analyze:bundle": "./scripts/analyze-bundle.sh",
    "deploy:preview": "./scripts/deploy-preview.sh",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:preview:all": "eas build --profile preview --platform all",
    "build:production:ios": "eas build --profile production --platform ios",
    "build:production:android": "eas build --profile production --platform android",
    "build:production:all": "eas build --profile production --platform all",
    "prepare": "husky install || echo 'Husky not installed'",
    "validate": "npm run typecheck && npm run format:check && npm run lint"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.2",
    "@sentry/react-native": "^7.100.0",
    "@shopify/flash-list": "^2.3.0",
    "@supabase/supabase-js": "^2.45.0",
    "expo": "~54.0.0",
    "expo-bluetooth": "^0.0.0",
    "expo-file-system": "^18.0.0",
    "expo-image": "~2.1.0",
    "expo-image-manipulator": "^15.0.0",
    "expo-image-picker": "^17.0.0",
    "expo-print": "^15.0.0",
    "expo-secure-store": "^14.0.0",
    "expo-sqlite": "^17.0.0",
    "expo-status-bar": "~2.0.0",
    "nativewind": "^4.2.1",
    "react": "19.0.0",
    "react-native": "0.79.0",
    "react-native-bluetooth-escpos-printer": "^0.0.5",
    "react-native-purchases": "^9.6.13",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.14.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.0.0",
    "jest": "^30.2.0",
    "jest-expo": "^55.0.0",
    "react-test-renderer": "^19.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.7.0"
  },
  "private": true
}
